{
  "name": "easy-vac",
  "version": "1.0.0",
  "description": "Validate-and-Clean made easy with TypeScript / JavaScript.",
  "author": "lyonbot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyonbot/easy-vac/issues"
  },
  "homepage": "https://lyonbot.github.io/easy-vac/",
  "main": "lib/index.js",
  "unpkg": "lib/index.umd.js",
  "module": "lib/index.es.js",
