    "rollup-plugin-typescript2": "^0.19.3",
    "tape": "^4.10.1",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "reflect-metadata": "*"
  }
}